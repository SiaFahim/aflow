{
  "manifest_version": 3,

  "name": "AFLOW",
  "description": "This extension makes the webpage breathe",
  "version": "0.1",
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["styles.css"],
      "js": ["jquery.js" , "flow.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"

  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
