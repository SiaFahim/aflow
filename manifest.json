{
  "manifest_version": 2,

  "name": "AFLOW",
  "description": "This extension makes the webpage breathe",
  "version": "0.1",
    "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["popup/styles.css" , "behaviour/styles.css"],
      "js": ["behaviour/jquery.js" , "behaviour/flow.js" , "popup/popup.js"]
    }
  ],

  "browser_action": {
    "default_icon": "popup/icon.png",
    "default_popup": "/popup/popup.html"

  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
